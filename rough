
<?php 
if($this->session->userdata('UserLoginSession'))
{

    $udata = $this->session->userdata('UserLoginSession');
    echo 'Welcome'.' '.$udata['username'];

}
else
{
    redirect(base_url('index.php/frontend/login'));
}
?>
